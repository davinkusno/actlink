{
    "version": 2,
    "builds": [
      { "src": "public/index.php", "use": "@vercel/php" },
      { "src": "resources/css/*", "use": "@vercel/static" },
      { "src": "resources/js/*", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "public/index.php" }
    ]
  }