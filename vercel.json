{
    "version": 2,
    "routes": [
      { "src": "/(.*)", "dest": "public/index.php" }
    ],
    "builds": [
      { "src": "public/index.php", "use": "vercel-php" }
    ],
    "scripts": {
      "postInstall": [
        "php artisan config:cache",
        "php artisan route:cache",
        "php artisan view:cache"
      ]
    }
  }
  