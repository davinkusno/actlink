{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/index.php"
    }
  ],
  "builds": [
    {
      "src": "public/index.php",
      "use": "@vercel/static-build"
    }
  ],
  "installCommand": "composer install --ignore-platform-reqs",
  "env": {
    "APP_STORAGE": "/tmp",
    "SESSION_DRIVER": "file",
    "CACHE_DRIVER": "file",
    "VIEW_COMPILED_PATH": "/tmp/views",
    "LOG_CHANNEL": "stderr"
  }
}
